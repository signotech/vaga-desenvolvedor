{
  "name": "signo_jobs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "project-up": "docker-compose up -d && npm run database:destroy && npm run database:init && npm run dev",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --coverage --verbose",
    "prettier:write": "prettier -w ./src",
    "prettier:check": "prettier -c ./src",
    "lint": "next lint",
    "database:init": "sequelize db:migrate && sequelize db:seed:all",
    "database:destroy": "sequelize db:seed:undo:all && sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@types/node": "20.8.4",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "bootstrap": "^5.3.2",
    "eslint": "8.51.0",
    "eslint-config-next": "13.5.4",
    "jsonwebtoken": "^9.0.2",
    "next": "13.5.4",
    "nookies": "^2.5.2",
    "pg": "^8.11.3",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "reactstrap": "^9.2.0",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.69.2",
    "sequelize": "^6.33.0",
    "sequelize-typescript": "^2.1.5",
    "sharp": "^0.32.6",
    "sinon": "^16.1.0",
    "typescript": "5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/sequelize": "^4.28.16",
    "@types/sinon": "^10.0.19",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.0",
    "jest-environment-jsdom": "^29.7.0",
    "sequelize-cli": "^6.6.1",
    "ts-jest": "^29.1.1"
  }
}
